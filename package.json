{
  "name": "animejs-builder",
  "version": "0.0.6",
  "description": "A sequential animation building tool for the Anime.js library",
  "repository": {
    "type": "git",
    "url": "https://github.com/lance13c/animejs-builder"
  },
  "browser": "lib/AnimeBuilder.iife.min.js",
  "umd:main": "lib/AnimeBuilder.min.js",
  "main": "lib/AnimeBuilder.js",
  "umd:name": "AnimeBuilder",
  "scripts": {
    "start": "./node_modules/.bin/parcel index.html",
    "build": "./node_modules/.bin/rollup -c rollup.config.js",
    "test": "./node_modules/.bin/mocha --compilers js:@babel/register"
  },
  "author": "Dominic Cicilio",
  "license": "MIT",
  "dependencies": {
    "animejs": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.11.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sass": "^1.16.1"
